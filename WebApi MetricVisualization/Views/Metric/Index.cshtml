
@{
    Layout = "_Layout";
    string url = "";
}

<h1></h1>

<script src="https://code.jquery.com/jquery-3.4.1.min.js"
        integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo="
        crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/chart.js@2.8.0/dist/Chart.min.js"></script>
<link rel="stylesheet" href="https://localhost:44367/css/fm.revealator.jquery.css" />
<script src="https://localhost:44367/js/fm.revealator.jquery.js"></script>



<body>
    <div class="container revealator-once revealator-load revealator-slideright revealator-delay3 revealator-duration10">
        <h1 class="revealator-once revealator-load  revealator-slidedown revealator-delay4 revealator-duration10">Available metrics</h1>
        @foreach (string result in ViewBag.result)
        {
            url = $"https://localhost:44367/api/metric/graph/{result}";
            <p class=""><a href="@url">@result</a></p>
        }
    </div>
</body>